<UserControl
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:UI.View"
             xmlns:ViewModel="clr-namespace:UI.ViewModel"
             xmlns:syncfusion="http://schemas.syncfusion.com/wpf" xmlns:telerik="http://schemas.telerik.com/2008/xaml/presentation" x:Class="UI.View.LiveCurrenciesView"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">

    <UserControl.Resources>
        <ViewModel:LiveViewModel x:Key="liveViewModel"/>
    </UserControl.Resources>
    <Grid DataContext="{StaticResource liveViewModel}">


        <syncfusion:SfDataGrid ItemsSource="{Binding liveCollection}"/>




    </Grid>
</UserControl>




